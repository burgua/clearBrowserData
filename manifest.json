{
    "name": "Toolbar button for clear browser data",
    "version": "0.0.2",
    "manifest_version": 2,
    "description": "Toolbar button for opening 'clear browser data' dialog",
    "permissions": [
        "tabs"
    ],
    "browser_action": {
        "default_icon": "icons/clean_48.png",
        "default_title": "Open 'clear browser data' dialog"
    },
	"background": {
		"scripts": ["scripts.js"]
	},
    "icons": {
        "48": "icons/clean_48.png",
		"64": "icons/clean_64.png",
        "128": "icons/clean_128.png"
    }
}
